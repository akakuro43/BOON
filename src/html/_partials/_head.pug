meta(property="og:url", content=data.meta.domain + data.meta.path + page.path)
meta(property="og:image", content=data.meta.domain + data.meta.path + '/assets/images/common/ogp.png')
meta(name="twitter:card", content="summary_large_image")
meta(name="twitter:title", content=titleMerge)
