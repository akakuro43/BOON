link(rel="stylesheet", href="/assets/css/main.css")
link(rel="icon" href=data.meta.path + "/assets/images/common/favicon.ico")
link(rel="apple-touch-icon" sizes="180x180" href=data.meta.path + "/assets/images/common/app_icon.png")
meta(property="og:title", content=titleMerge)
meta(property="og:site_name", content=data.meta.websiteName)
meta(property="og:type", content="website")
