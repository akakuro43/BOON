mixin svg(width,widthBase, heightBase, classNameBase, className, id, isVW)
  if isVW
    svg(
      xmlns = "http://www.w3.org/2000/svg",
      width = width + 'vw',
      height = Math.ceil(width * heightBase / widthBase) + 'vw',
      viewBox = `0 0 ${widthBase} ${heightBase}`,
      class = (className) ? `${classNameBase} ${className}` : classNameBase,
      id = (id) ? id : '',
    )
      block
  else
    svg(
      xmlns = "http://www.w3.org/2000/svg",
      width = width,
      height = Math.ceil(width * heightBase / widthBase),
      viewBox = `0 0 ${widthBase} ${heightBase}`,
      class = (className) ? `${classNameBase} ${className}` : classNameBase,
      id = (id) ? id : '',
    )
      block


